INSERT INTO POSTSS (USER_ID, BODY)VALUES (4, 'Yo all! Here is Thomas!');

COMMIT;

CREATE TABLE ISSUESLISTS(
    ID   SERIAL PRIMARY KEY,
    NAME VARCHAR(100) CHARSET utf8
);

CREATE TABLE ISSUES(
    ID SERIAL PRIMARY KEY,
    ISSUESLIST_ID BIGINT UNSIGNED NOT NULL,
    SUMMARY VARCHAR(500) CHARSET utf8,
    DESCRIPTION VARCHAR(1000) CHARSET utf8,
    USER_ID_ASSIGNEDTO BIGINT UNSIGNED NOT NULL,
    FOREIGN KEY(ISSUESLIST_ID) REFERENCES ISSUESLISTS(ID),
    FOREIGN KEY (USER_ID_ASSIGNEDTO) REFERENCES USERSS(ID)
);

