CREATE TABLE USERSS (
    ID SERIAL PRIMARY KEY,
    FIRSTNAME VARCHAR(100),
    LASTNAME VARCHAR(100)
);

CREATE TABLE POSTSS (
    ID  SERIAL PRIMARY KEY,
    USER_ID  BIGINT UNSIGNED NOT NULL,
    BODY VARCHAR(1024),
    FOREIGN KEY (USER_ID) REFERENCES USERSS(ID)
);

SELECT * FROM USERSS;

INSERT INTO USERSS (FIRSTNAME, LASTNAME) VALUES ('John', 'Smith');

COMMIT;

INSERT INTO POSTSS (USER_ID, BODY) VALUES (1, 'This is my first post on this forum!');

COMMIT;

SELECT * FROM POSTSS;

INSERT INTO USERSS (FIRSTNAME, LASTNAME)VALUES ('Zachary', 'Lee');

